{"ast":null,"code":"import axios from 'axios';\nimport AlertComponent from '@/components/AlertComponent.vue';\nimport CardFormComponent from '@/components/cards/CardFormComponent.vue';\nimport ButtonComponent from '@/components/ButtonComponent.vue';\nexport default {\n  data() {\n    return {\n      days_list: [{\n        id: 1,\n        name: 'ПН'\n      }, {\n        id: 2,\n        name: 'ВТ'\n      }, {\n        id: 3,\n        name: 'СР'\n      }, {\n        id: 4,\n        name: 'ЧТ'\n      }, {\n        id: 5,\n        name: 'ПТ'\n      }, {\n        id: 6,\n        name: 'СБ'\n      }, {\n        id: 7,\n        name: 'ВС'\n      }],\n      user_info: null,\n      alert: '',\n      lesson_id: null,\n      day_id: null,\n      group_id: null,\n      timeTo: null,\n      timeDo: null,\n      lesson_list: null,\n      group_list: null,\n      timedata_list: null\n    };\n  },\n\n  props: {\n    is_auth: Number,\n    token: String\n  },\n  components: {\n    ButtonComponent,\n    CardFormComponent,\n    AlertComponent\n  },\n  methods: {\n    user: function () {\n      if (this.is_auth >= 0) {\n        axios.get('http://localhost:5000/api/user/user-info?access_token=' + this.token, {\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        }).then(response => {\n          this.user_info = response.data[0].message; // Вызывать методы использующие пользовательские данные ниже\n\n          this.lesson_show(this.user_info.organization_id);\n          this.all_group(this.user_info.organization_id);\n          this.user_type = this.user_info.role;\n        }).catch(function (error) {\n          console.log(error);\n        });\n      }\n    },\n\n    all_group(organizationId) {\n      axios.get('http://localhost:5000/api/group/all-group?organization_id=' + organizationId, {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*'\n        }\n      }).then(response => {\n        this.group_list = response.data[0].message.list;\n      });\n    },\n\n    lesson_show(id) {\n      // Запрос на выборку данных юзера по ролям\n      axios.get('http://localhost:5000/api/lesson/lesson-all?organization_id=' + id, {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*'\n        }\n      }).then(response => {\n        this.lesson_list = response.data[0].message.list;\n      }).catch(function (error) {\n        if (error.error) {\n          // Деактивация лоадера\n          document.getElementById('loader-bg').style.display = 'none';\n          self.alert = 'Ошибка получения данных!'; // Активация всплывающего сообщения\n\n          document.getElementById('toast').style.opacity = 1;\n        }\n      }); // END\n    },\n\n    group_activate(idx, item) {\n      for (var element = 0; element < this.$refs.groupList.length; element++) {\n        this.$refs.groupList[element].classList.remove('active');\n      } // Активация кнопки + сохранение выбраного значения\n\n\n      this.$refs.groupList[idx].classList.add('active');\n      this.group_id = item; // Подгрузка данных группы в таблицу\n\n      this.show_timetable_on_group(this.user_info.organization_id, this.group_id);\n    },\n\n    day_activate(idx, item) {\n      for (var element = 0; element < this.$refs.dayList.length; element++) {\n        this.$refs.dayList[element].classList.add('day-no');\n      }\n\n      this.$refs.dayList[idx].classList.remove('day-no');\n      this.day_id = item;\n    },\n\n    show_timetable_on_group(organizationId, groupId) {\n      // Запрос на выборку данных юзера по ролям\n      axios.get('http://localhost:5000/api/timetable/show-timetable?organization_id=' + organizationId + '&&groups_id=' + groupId, {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*'\n        }\n      }).then(response => {\n        var arr = response.data[0].message;\n        this.timedata_list = arr.sort(byField('day'));\n        console.log(this.timedata_list);\n      }).catch(function (error) {\n        if (error.error) {\n          self.alert = 'Ошибка получения данных!'; // Активация всплывающего сообщения\n\n          document.getElementById('toast').style.opacity = 1;\n        }\n      }); // END\n    },\n\n    save() {\n      // Отправка данных\n      axios.post('http://localhost:5000/api/timetable/create-timetable', {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*',\n          'Access-Control-Allow-Headers': 'Origin, Content-Type, X-Auth-Token, Authorization, Accept,charset,boundary,Content-Length'\n        },\n        organization_id: this.user_info.organization_id,\n        lesson_id: this.lesson_id,\n        groups_id: this.group_id,\n        day: this.day_id,\n        time_start: this.timeTo,\n        time_end: this.timeDo\n      }).then(response => {\n        this.alert = 'Данные успешно сохранены'; // Активация всплывающего сообщения\n\n        document.getElementById('toast').style.opacity = 1; // Деактивация лоадера\n\n        document.getElementById('loader-bg').style.display = 'none'; // Очистка полей\n\n        this.lesson_id = '';\n        this.group_id = '';\n        this.day_id = '';\n        this.timeTo = '';\n        this.timeDo = '';\n      }).catch(function (error) {\n        if (error.response) {\n          // Деактивация лоадера\n          document.getElementById('loader-bg').style.display = 'none';\n          self.alert = 'Ошибка сохранения данных!'; // Активация всплывающего сообщения\n\n          document.getElementById('toast').style.opacity = 1;\n        }\n      }); // END\n\n      console.log({\n        organization_id: this.user_info.organization_id,\n        lesson_id: this.lesson_id,\n        groups_id: this.group_id,\n        day: this.day_id,\n        time_start: this.timeTo,\n        time_end: this.timeDo\n      });\n    }\n\n  },\n\n  mounted() {\n    this.user();\n  }\n\n};","map":{"version":3,"mappings":"AAuIA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,iBAAP,MAA8B,0CAA9B;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CADS,EAET;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAFS,EAGT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAHS,EAIT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAJS,EAKT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CALS,EAMT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CANS,EAOT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAPS,CADN;MAULC,SAAS,EAAE,IAVN;MAWLC,KAAK,EAAE,EAXF;MAYLC,SAAS,EAAE,IAZN;MAaLC,MAAM,EAAE,IAbH;MAcLC,QAAQ,EAAE,IAdL;MAeLC,MAAM,EAAE,IAfH;MAgBLC,MAAM,EAAE,IAhBH;MAiBLC,WAAW,EAAE,IAjBR;MAkBLC,UAAU,EAAE,IAlBP;MAmBLC,aAAa,EAAE;IAnBV,CAAP;EAqBD,CAvBY;;EAwBbC,KAAK,EAAE;IACLC,OAAO,EAAEC,MADJ;IAELC,KAAK,EAAEC;EAFF,CAxBM;EA4BbC,UAAU,EAAE;IACVpB,eADU;IAEVD,iBAFU;IAGVD;EAHU,CA5BC;EAiCbuB,OAAO,EAAE;IACPC,IAAI,EAAE,YAAY;MAChB,IAAI,KAAKN,OAAL,IAAgB,CAApB,EAAuB;QACrBnB,KAAK,CAAC0B,GAAN,CAAU,2DAA2D,KAAKL,KAA1E,EACE;UACEM,OAAO,EAAE;YACP,gBAAgB;UADT;QADX,CADF,EAOGC,IAPH,CAOSC,QAAD,IAAc;UAClB,KAAKrB,SAAL,GAAiBqB,QAAQ,CAACzB,IAAT,CAAc,CAAd,EAAiB0B,OAAlC,CADkB,CAElB;;UACA,KAAKC,WAAL,CAAiB,KAAKvB,SAAL,CAAewB,eAAhC;UACA,KAAKC,SAAL,CAAe,KAAKzB,SAAL,CAAewB,eAA9B;UACA,KAAKE,SAAL,GAAiB,KAAK1B,SAAL,CAAe2B,IAAhC;QACD,CAbH,EAcGC,KAdH,CAcS,UAAUC,KAAV,EAAiB;UACtBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACD,CAhBH;MAiBF;IACD,CArBM;;IAsBPJ,SAAQ,CAAGO,cAAH,EAAmB;MACzBxC,KAAK,CAAC0B,GAAN,CAAU,+DAA+Dc,cAAzE,EACE;QACEb,OAAO,EAAE;UACP,gBAAgB,kBADT;UAEP,+BAA+B;QAFxB;MADX,CADF,EAOGC,IAPH,CAOSC,QAAD,IAAc;QAClB,KAAKb,UAAL,GAAkBa,QAAQ,CAACzB,IAAT,CAAc,CAAd,EAAiB0B,OAAjB,CAAyBW,IAA3C;MACD,CATH;IAUD,CAjCM;;IAkCPV,WAAU,CAAGzB,EAAH,EAAO;MACf;MACAN,KAAK,CAAC0B,GAAN,CAAU,iEAAiEpB,EAA3E,EACE;QACEqB,OAAO,EAAE;UACP,gBAAgB,kBADT;UAEP,+BAA+B;QAFxB;MADX,CADF,EAQGC,IARH,CAQSC,QAAD,IAAc;QAClB,KAAKd,WAAL,GAAmBc,QAAQ,CAACzB,IAAT,CAAc,CAAd,EAAiB0B,O