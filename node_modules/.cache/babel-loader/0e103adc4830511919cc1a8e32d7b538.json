 text=\"ПТ\"\n                      css_class=\"btn mt-2 right\"\n                    />\n                    <!-- END -->\n                  </div>\n                  <div class=\"col-1\">\n                    <!-- Компонент кнопки -->\n                    <ButtonComponent\n                      text=\"СБ\"\n                      css_class=\"btn mt-2 right\"\n                    />\n                    <!-- END -->\n                  </div>\n                  <div class=\"col-1\">\n                    <!-- Компонент кнопки -->\n                    <ButtonComponent\n                      text=\"ВС\"\n                      css_class=\"btn mt-2 right\"\n                    />\n                    <!-- END -->\n                  </div>\n                </div>\n                <hr>\n                <div class=\"col-8 mx-auto mt-3\">\n                  <form>\n                    <div class=\"mb-3\">\n                      <label class=\"form-label\">\n                        <b>Выберите предмет</b>\n                      </label>\n                      <select class=\"form-select\" v-model=\"lesson_name\" aria-label=\"Default select example\">\n                        <option selected>Математика</option>\n                      </select>\n                    </div>\n                    <div class=\"row\">\n                      <div class=\"mb-3 col-6\">\n                        <label class=\"form-label\">\n                          <b>Время с:</b>\n                        </label>\n                        <input type=\"time\" v-model=\"timeTo\" class=\"form-control\">\n                      </div>\n                      <div class=\"mb-3 col-6\">\n                        <label class=\"form-label\">\n                          <b>Время до:</b>\n                        </label>\n                        <input type=\"time\" v-model=\"timeDo\" class=\"form-control\">\n                      </div>\n                    </div>\n                    <div class=\"mb-3\">\n      