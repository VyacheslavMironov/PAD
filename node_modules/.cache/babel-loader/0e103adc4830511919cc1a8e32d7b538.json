{"ast":null,"code":"import axios from 'axios';\nimport AlertComponent from '@/components/AlertComponent.vue';\nimport CardFormComponent from '@/components/cards/CardFormComponent.vue';\nimport ButtonComponent from '@/components/ButtonComponent.vue';\nexport default {\n  data() {\n    return {\n      users: null,\n      alert: '',\n      lesson_name: null,\n      lesson_list: null,\n      group_list: null\n    };\n  },\n\n  props: {\n    is_auth: Number,\n    token: String\n  },\n  components: {\n    ButtonComponent,\n    CardFormComponent,\n    AlertComponent\n  },\n  methods: {\n    user: function () {\n      if (this.is_auth >= 0) {\n        axios.get('http://localhost:5000/api/user/user-info?access_token=' + this.token, {\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        }).then(response => {\n          this.user_info = response.data[0].message; // Вызывать методы использующие пользовательские данные ниже\n\n          this.lesson_show(this.user_info.organization_id);\n          this.all_group(this.user_info.organization_id);\n          this.user_type = this.user_info.role;\n        }).catch(function (error) {\n          console.log(error);\n        });\n      }\n    },\n\n    all_group(organizationId) {\n      axios.get('http://localhost:5000/api/group/all-group?organization_id=' + organizationId, {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*'\n        }\n      }).then(response => {\n        this.group_list = response.data[0].message.list;\n      });\n    },\n\n    lesson_show(id) {\n      // Запрос на выборку данных юзера по ролям\n      axios.get('http://localhost:5000/api/lesson/lesson-all?organization_id=' + id, {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*'\n        }\n      }).then(response => {\n        this.lesson_list = response.data[0].message.list;\n      }).catch(function (error) {\n        if (error.error) {\n          // Деактивация лоадера\n          document.getElementById('loader-bg').style.display = 'none';\n          self.alert = 'Ошибка получения данных!'; // Активация всплывающего сообщения\n\n          document.getElementById('toast').style.opacity = 1;\n        }\n      }); // END\n    },\n\n    group_activate(idx) {\n      for (var element in this.$refs.groupList) {\n        element.classList.remove('active');\n      }\n\n      this.$refs.groupList[idx].classList.add('active');\n    }\n\n  },\n\n  mounted() {\n    this.user();\n  }\n\n};","map":{"version":3,"mappings":"AA6KA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,iBAAP,MAA8B,0CAA9B;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAE,IADF;MAELC,KAAK,EAAE,EAFF;MAGLC,WAAW,EAAE,IAHR;MAILC,WAAW,EAAE,IAJR;MAKLC,UAAU,EAAE;IALP,CAAP;EAOD,CATY;;EAUbC,KAAK,EAAE;IACLC,OAAO,EAAEC,MADJ;IAELC,KAAK,EAAEC;EAFF,CAVM;EAcbC,UAAU,EAAE;IACVZ,eADU;IAEVD,iBAFU;IAGVD;EAHU,CAdC;EAmBbe,OAAO,EAAE;IACPC,IAAI,EAAE,YAAY;MAChB,IAAI,KAAKN,OAAL,IAAgB,CAApB,EAAuB;QACrBX,KAAK,CAACkB,GAAN,CAAU,2DAA2D,KAAKL,KAA1E,EACE;UACEM,OAAO,EAAE;YACP,gBAAgB;UADT;QADX,CADF,EAOGC,IAPH,CAOSC,QAAD,IAAc;UAClB,KAAKC,SAAL,GAAiBD,QAAQ,CAACjB,IAAT,CAAc,CAAd,EAAiBmB,OAAlC,CADkB,CAElB;;UACA,KAAKC,WAAL,CAAiB,KAAKF,SAAL,CAAeG,eAAhC;UACA,KAAKC,SAAL,CAAe,KAAKJ,SAAL,CAAeG,eAA9B;UACA,KAAKE,SAAL,GAAiB,KAAKL,SAAL,CAAeM,IAAhC;QACD,CAbH,EAcGC,KAdH,CAcS,UAAUC,KAAV,EAAiB;UACtBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACD,CAhBH;MAiBF;IACD,CArBM;;IAsBPJ,SAAQ,CAAGO,cAAH,EAAmB;MACzBjC,KAAK,CAACkB,GAAN,CAAU,+DAA+De,cAAzE,EACE;QACEd,OAAO,EAAE;UACP,gBAAgB,kBADT;UAEP,+BAA+B;QAFxB;MADX,CADF,EAOGC,IAPH,CAOSC,QAAD,IAAc;QAClB,KAAKZ,UAAL,GAAkBY,QAAQ,CAACjB,IAAT,CAAc,CAAd,EAAiBmB,OAAjB,CAAyBW,IAA3C;MACD,CATH;IAUD,CAjCM;;IAkCPV,WAAU,CAAGW,EAAH,EAAO;MACf;MACAnC,KAAK,CAACkB,GAAN,CAAU,iEAAiEiB,EAA3E,EACE;QACEhB,OAAO,EAAE;UACP,gBAAgB,kBADT;UAEP,+BAA+B;QAFxB;MADX,CADF,EAQGC,IARH,CAQSC,QAAD,IAAc;QAClB,KAAKb,WAAL,GAAmBa,QAAQ,CA