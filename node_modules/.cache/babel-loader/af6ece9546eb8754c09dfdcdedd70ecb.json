ganization_id=' + organizationId + '&&groups_id=' + groupId, {\n        headers: {\n          'Content-Type': 'application/json',\n          'Access-Control-Allow-Origin': '*'\n        }\n      }).then(response => {\n        this.timedata_list = this.days(response.data[0].message.list);\n        console.log(this.$refs.dropTimetable);\n      }).catch(function (error) {\n        if (error.error) {\n          self.alert = 'Ошибка получения данных!'; // Активация всплывающего сообщения\n\n          document.getElementById('toast').style.opacity = 1;\n        }\n      }); // END\n    }\n\n  },\n\n  mounted() {\n    this.user();\n  }\n\n};","map":{"version":3,"mappings":"AA2GA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,iBAAP,MAA8B,0CAA9B;AACA,OAAOC,eAAP,MAA4B,kCAA5B;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CADS,EAET;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAFS,EAGT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAHS,EAIT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAJS,EAKT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CALS,EAMT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CANS,EAOT;QAAED,EAAE,EAAE,CAAN;QAASC,IAAI,EAAE;MAAf,CAPS,CADN;MAULC,SAAS,EAAE,IAVN;MAWLC,KAAK,EAAE,EAXF;MAYLC,SAAS,EAAE,IAZN;MAaLC,MAAM,EAAE,IAbH;MAcLC,QAAQ,EAAE,IAdL;MAeLC,MAAM,EAAE,IAfH;MAgBLC,MAAM,EAAE,IAhBH;MAiBLC,WAAW,EAAE,IAjBR;MAkBLC,UAAU,EAAE,IAlBP;MAmBLC,aAAa,EAAE;IAnBV,CAAP;EAqBD,CAvBY;;EAwBbC,KAAK,EAAE;IACLC,OAAO,EAAEC,MADJ;IAELC,KAAK,EAAEC;EAFF,CAxBM;EA4BbC,UAAU,EAAE;IACVrB,iBADU;IAEVD,cAFU;IAGVE;EAHU,CA5BC;EAiCbqB,OAAO,EAAE;IACPC,IAAI,EAAE,YAAY;MAChB,IAAI,KAAKN,OAAL,IAAgB,CAApB,EAAuB;QACrBnB,KAAK,CAAC0B,GAAN,CAAU,2DAA2D,KAAKL,KAA1E,EACE;UACEM,OAAO,EAAE;YACP,gBAAgB;UADT;QADX,CADF,EAOGC,IAPH,CAOSC,QAAD,IAAc;UAClB,KAAKrB,SAAL,GAAiBqB,QAAQ,CAACzB,IAAT,CAAc,CAAd,EAAiB0B,OAAlC,CADkB,CAElB;;UACA,KAAKC,WAAL,CAAiB,KAAKvB,SAAL,CAAewB,eAAhC;UACA,KAAKC,SAAL,CAAe,KAAKzB,SAAL,CAAewB,eAA9B;UACA,KAAKE,SAAL,GAAiB,KAAK1B,SAAL,CAAe2B,IAAhC;QACD